    <PackageReference Include="Fake.Core.Target" />
    <PackageReference Include="Fake.Documentation.DocFx" />
    <PackageReference Include="Fake.DotNet.Cli" />
    <PackageReference Include="Fake.Tools.Git" />
    <PackageReference Update="FSharp.Core" GeneratePathProperty="true" PrivateAssets="all" />
  </ItemGroup>

</Project>
